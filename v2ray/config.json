{
  "log": {
    "loglevel": "warning"
  },

  "inbounds": [{
    "port": "${PORT}",
    "listen": "127.0.0.1",
    "protocol": "http"
  }],

  "outbounds": [
    {
      "protocol": "http",
      "settings": {
        "servers": [
          {
            "address": "${PROXY_HOST}",
            "port": "${PROXY_PORT}",
            "users": [
              {
                "user": "${PROXY_USERNAME}",
                "pass": "${PROXY_PASSWORD}"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls"
      },
      "mux": {
        "enabled": false,
        "concurrency": -1
      }
    }
  ]
}
